{
  "name": "username_generator",
  "description": "A web application that creates a username based on the information you provide it.",
  "version": "1.0.0",
  "homepage": "https://github.com/CF711/username_generator",
  "private": false,
  "devDependencies": {
    "bower": "^1.7.7",
    "http-server": "^0.9.0"
  },
  "scripts": {
    "postinstall": "bower install",
    "update-deps": "npm update",
    "postupdate-deps": "bower update",
    "prestart": "npm install",
    "start": "http-server -a localhost -p 8100 -c-1 ./app",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update"
  }
}
